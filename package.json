{
  "name": "@fallingfish/dbds",
  "version": "1.9.3",
  "description": "TypeScript type generator for PostgreSQL databases",
  "keywords": [
    "sql",
    "postgresql",
    "typescript",
    "generation",
    "typings",
    "graphql",
    "apollo-server",
    "dataloader",
    "dao"
  ],
  "homepage": "https://github.com/ProjectXero/dbds",
