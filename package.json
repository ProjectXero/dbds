    "@databases/pg-test": "^3.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/config": "^0.0.38",
    "@types/jest": "^26.0.18",
    "@types/node": "^16.11.2",
