    "husky": "^5.0.0",
    "jest": "^26.6.3",
    "pinst": "^2.1.1",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "typescript": ">= 3.9.0"
